<div class="container">
	<div class="container" style="margin-top: 20px">
		<div class="col-lg-2">
			<div class="panel panel-default">
				<div class="panel-heading">All Colleges</div>
				<div class="panel-body">
					<ul>
						
						<li><%= link_to 'Back', admin_dashboard_path %></li>
					</ul>
				</div>
			</div>
			
		</div>
		<div class="col-lg-10 text-center">
			
			<h2>
			<%= @college.name %>
			</h2>
			<%= image_tag @college.photo.url(:main), :class => "college-feature" %>
			<p class="text-left">
				<%= @college.summary %>
			</p>
		</div>
	</div>
</div>